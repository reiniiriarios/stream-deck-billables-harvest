      "UUID": "me.reinii.harvest-billables.timer-total",
      "Icon": "images/icon-default",
      "States": [
        {
          "Image": "images/icon-default"
        }
      ],
      "PropertyInspectorPath": "timer-total-pi.html"
    },
    {
      "Name": "Billables Status",
      "Controllers": [
        "Keypad"
      ],
      "Tooltip": "Show current billables status.",
      "UUID": "me.reinii.harvest-billables.status",
      "Icon": "images/icon-default",
      "States": [
        {
          "Image": "images/icon-default"
        }
      ],
      "PropertyInspectorPath": "status-pi.html"
    }
