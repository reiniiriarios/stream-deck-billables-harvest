{
  "SDKVersion": 2,
  "Author": "Emma Litwa-Vulcu",
  "CodePath": "app.html",
  "Description": "Track time in Harvest and display time tracked compared with billable hours assigned in Forecast.",
  "Category": "Harvest Time Tracking",
  "CategoryIcon": "images/icon-category",
  "Name": "Harvest Time Tracking & Billables",
  "Icon": "images/icon-default",
  "URL": "https://github.com/reiniiriarios/stream-deck-billables-harvest/",
  "Version": "0.4.0",
  "Software": {
    "MinimumVersion": "5.0"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.11"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Actions": [
    {
      "Name": "Timer",
      "Controllers": [
        "Keypad"
      ],
